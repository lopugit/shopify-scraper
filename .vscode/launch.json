	"version": "0.2.0",
	"configurations": [
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Current File",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${file}"
		},
		{
			"type": "pwa-node",
			"request": "launch",
			"name": "Index",
			"skipFiles": [
				"<node_internals>/**"
			],
			"program": "${workspaceFolder}/node/index.js"
		}
	]
}